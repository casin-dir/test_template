{
  "name": "space-pioneer",
  "version": "0.1.0",
  "license": "Propriate",
  "scripts": {
    "clean": "rimraf public",
    "video:build:all": "NUM=2 FPS=25 npm run video:build:num && NUM=4 FPS=25 npm run video:build:num && NUM=6 FPS=25 npm run video:build:num",
    "video:build:num": "NUM=${NUM} npm run video:clean:num && ffmpeg -i src/video/video${NUM}_1080p.mp4 -qscale:v 1 -vf scale=-1:1080 -r ${FPS} src/video/frames${NUM}/%d.jpg;",
    "video:clean:num": "cd src/video; rimraf frames${NUM} && mkdir -p frames${NUM}",
    "build": "npm run clean && NODE_ENV=production VIEW=fullscreen webpack && NODE_ENV=production VIEW=banner webpack",
    "build:fullscreen": "npm run clean && NODE_ENV=production VIEW=fullscreen webpack",
    "build:banner": "npm run clean && NODE_ENV=production VIEW=banner webpack",
    "server:fullscreen": "npm run clean && VIEW=fullscreen babel-node webpack.server.js",
    "server:banner": "npm run clean && VIEW=banner babel-node webpack.server.js",
    "zip": "cd public; zip -9 -r ${MODE}.zip ${MODE}/",
    "zip:fullscreen": "MODE=fullscreen npm run zip",
    "zip:banner": "MODE=banner npm run zip",
    "zip:all": "npm run zip:banner && npm run zip:fullscreen",
    "lint": "eslint . --ext=.js,.jsx"
  },
  "commands": [
    "yarn build",
    "yarn zip:all",
    "cp mailru.html public/"
  ],
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-check-es2015-constants": "6.8.0",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-transform-async-functions": "6.8.0",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-arrow-functions": "6.8.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "6.18.0",
    "babel-plugin-transform-es2015-classes": "6.18.0",
